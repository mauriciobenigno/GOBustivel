<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".TelasTeste.TelaCadastroPosto">

    <EditText
            android:layout_width="78dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/campoDVusr"
            android:text="V. Usr"
            app:layout_constraintStart_toEndOf="@+id/campoDSVmax" android:layout_marginStart="20dp"
            android:layout_marginTop="24dp" app:layout_constraintTop_toBottomOf="@+id/textView4"/>
    <TextView
            android:text="DIESEL S10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:textSize="24sp" app:layout_constraintHorizontal_bias="0.498" android:layout_marginTop="80dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3"/>
    <EditText
            android:layout_width="78dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/campoDSVmax"
            android:text="V. Max"
            app:layout_constraintStart_toEndOf="@+id/campoDSVmed" android:layout_marginStart="20dp"
            android:layout_marginTop="24dp" app:layout_constraintTop_toBottomOf="@+id/textView4"/>
    <EditText
            android:layout_width="78dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/campoDSVmin"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
            android:text="V. Min." android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/textView4"/>
    <TextView
            android:text="TELA CADASTRO"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:textSize="24sp"
    />
    <EditText
            android:layout_width="333dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Nome do Posto"
            android:ems="10"
            android:id="@+id/campoNome" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.467" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/campoRzSocial"/>
    <EditText
            android:layout_width="335dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Cidade"
            android:ems="10"
            android:id="@+id/campoCidade" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/campoNome" app:layout_constraintHorizontal_bias="0.483"/>
    <EditText
            android:layout_width="335dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Bairro"
            android:ems="10"
            android:id="@+id/campoBairro" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/campoCidade" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.483"/>
    <EditText
            android:layout_width="338dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/campoRzSocial" android:layout_marginEnd="40dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2"/>
    <TextView
            android:text="DIESEL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/campoBairro" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:textSize="24sp" app:layout_constraintHorizontal_bias="0.498"/>
    <EditText
            android:layout_width="78dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/campoDVmin"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/textView3"
            android:text="V. Min."/>
    <EditText
            android:layout_width="78dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/campoDVmed"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/textView3"
            android:text="V. Med." app:layout_constraintStart_toEndOf="@+id/campoDVmin"
            android:layout_marginStart="12dp"/>
    <EditText
            android:layout_width="78dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/campoDVmax"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/textView3"
            android:text="V. Max"
            app:layout_constraintStart_toEndOf="@+id/campoDVmed" android:layout_marginStart="16dp"/>
    <EditText
            android:layout_width="78dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/campoDSVusr"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/textView3"
            android:text="V. Usr"
            app:layout_constraintStart_toEndOf="@+id/campoDVmax" android:layout_marginStart="12dp"/>
    <EditText
            android:layout_width="78dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/campoDSVmed"
            android:text="V. Med."
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/textView4" app:layout_constraintStart_toEndOf="@+id/campoDSVmin"
            android:layout_marginStart="12dp"/>
    <ScrollView
            android:layout_width="395dp"
            android:layout_height="179dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.985"
            android:id="@+id/scrollView2">
        <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                      android:orientation="horizontal">
            <EditText
                    android:layout_width="75dp"
                    android:layout_height="wrap_content"
                    android:inputType="number"
                    android:ems="10"
                    tools:layout_editor_absoluteY="309dp" android:id="@+id/editText10"
                    app:layout_constraintStart_toEndOf="@+id/editText9" android:layout_marginStart="12dp"/>
            <EditText
                    android:layout_width="78dp"
                    android:layout_height="wrap_content"
                    android:inputType="number"
                    android:ems="10"
                    android:id="@+id/editText9"
                    app:layout_constraintStart_toEndOf="@+id/editText8" android:layout_marginStart="20dp"
                    android:layout_marginTop="24dp" app:layout_constraintTop_toBottomOf="@+id/textView5"/>
            <EditText
                    android:layout_width="74dp"
                    android:layout_height="wrap_content"
                    android:inputType="number"
                    android:ems="10"
                    android:id="@+id/editText8"
                    app:layout_constraintStart_toEndOf="@+id/editText7" android:layout_marginStart="16dp"
                    android:layout_marginTop="24dp" app:layout_constraintTop_toBottomOf="@+id/textView5"/>
            <EditText
                    android:layout_width="73dp"
                    android:layout_height="wrap_content"
                    android:inputType="number"
                    android:ems="10"
                    android:id="@+id/editText7" android:layout_marginTop="24dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView5" android:layout_marginStart="32dp"
                    app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                    android:text="ETANOL COMUM"
                    android:layout_width="186dp"
                    android:layout_height="29dp"
                    android:id="@+id/textView5"
                    android:textSize="24sp" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    android:layout_marginTop="24dp" app:layout_constraintTop_toTopOf="@+id/scrollView2"/>
        </LinearLayout>
    </ScrollView>
</android.support.constraint.ConstraintLayout>